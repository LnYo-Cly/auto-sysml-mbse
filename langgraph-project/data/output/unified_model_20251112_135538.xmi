          <region xmi:id="region-air-con-main-uuid" name="主区域" xmi:type="uml:Region">
            <subvertex xmi:id="state-off-uuid" name="关闭" xmi:type="uml:State"/>
            <subvertex xmi:id="state-standby-uuid" name="待机" xmi:type="uml:State"/>
            <subvertex xmi:id="state-running-uuid" name="运行" xmi:type="uml:State"/>
