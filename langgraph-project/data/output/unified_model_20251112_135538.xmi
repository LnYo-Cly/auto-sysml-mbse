            <transition xmi:id="trans-initial-to-off-uuid" xmi:type="uml:Transition" source="ps-main-initial-uuid" target="state-off-uuid"/>
            <transition xmi:id="trans-off-to-standby-uuid" xmi:type="uml:Transition" source="state-off-uuid" target="state-standby-uuid"/>
            <transition xmi:id="trans-standby-to-running-uuid" xmi:type="uml:Transition" source="state-standby-uuid" target="state-running-uuid"/>
            <transition xmi:id="trans-running-to-off-uuid" xmi:type="uml:Transition" source="state-running-uuid" target="state-off-uuid"/>
