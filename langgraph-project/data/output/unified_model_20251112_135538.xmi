        <fragment xmi:id="fragment-send-sendtemp-uuid" xmi:type="uml:MessageOccurrenceSpecification" covered="ll-touchscreen-uuid" message="msg-sendtemp-uuid"/>
        <fragment xmi:id="fragment-recv-sendtemp-uuid" xmi:type="uml:MessageOccurrenceSpecification" covered="ll-aircondsys-uuid" message="msg-sendtemp-uuid"/>
        <message xmi:id="msg-adjustsys-uuid" name="调整系统" xmi:type="uml:Message" sendEvent="fragment-send-adjustsys-uuid" receiveEvent="fragment-recv-adjustsys-uuid" messageSort="synchCall"/>
        <fragment xmi:id="fragment-send-adjustsys-uuid" xmi:type="uml:MessageOccurrenceSpecification" covered="ll-aircondsys-uuid" message="msg-adjustsys-uuid"/>
        <fragment xmi:id="fragment-recv-adjustsys-uuid" xmi:type="uml:MessageOccurrenceSpecification" covered="ll-aircondsys-uuid" message="msg-adjustsys-uuid"/>
        <message xmi:id="msg-sendtempandhum-uuid" name="发送温度和湿度信息" xmi:type="uml:Message" sendEvent="fragment-send-sendtempandhum-uuid" receiveEvent="fragment-recv-sendtempandhum-uuid" messageSort="synchCall"/>
        <fragment xmi:id="fragment-send-sendtempandhum-uuid" xmi:type="uml:MessageOccurrenceSpecification" covered="ll-aircondsys-uuid" message="msg-sendtempandhum-uuid"/>
      </packagedElement>
