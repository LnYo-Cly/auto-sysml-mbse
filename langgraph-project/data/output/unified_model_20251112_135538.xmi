    <packagedElement xmi:id="pkg-unique-id" name="ParametricDiagram" xmi:type="uml:Package">
      <packagedElement xmi:id="block1" name="TemperatureControlSystem" xmi:type="uml:Class">
        <ownedAttribute xmi:id="prop1" name="T_current" xmi:type="uml:Property" visibility="public" aggregation="none">
          <type href="http://www.omg.org/spec/SysML/20181001/SysML.xmi#SysML_dataType.Real"/>
        </ownedAttribute>
        <ownedAttribute xmi:id="prop2" name="T_target" xmi:type="uml:Property" visibility="public" aggregation="none">
          <type href="http://www.omg.org/spec/SysML/20181001/SysML.xmi#SysML_dataType.Real"/>
        </ownedAttribute>
        <ownedAttribute xmi:id="prop3" name="Q" xmi:type="uml:Property" visibility="public" aggregation="none">
          <type href="http://www.omg.org/spec/SysML/20181001/SysML.xmi#SysML_dataType.Real"/>
        </ownedAttribute>
        <ownedAttribute xmi:id="prop4" name="t" xmi:type="uml:Property" visibility="public" aggregation="none">
          <type href="http://www.omg.org/spec/SysML/20181001/SysML.xmi#SysML_dataType.Real"/>
        </ownedAttribute>
        <ownedAttribute xmi:id="cp1" name="TemperatureControlEquation" xmi:type="uml:Property" visibility="public" aggregation="none" type="cb1"/>
        <ownedConnector xmi:id="conn1" xmi:type="uml:Connector">
          <end xmi:id="_conn1_end_1" xmi:type="uml:ConnectorEnd" role="prop1"/>
          <end xmi:id="_conn1_end_2" xmi:type="uml:ConnectorEnd" role="param1" partWithPort="cp1"/>
        </ownedConnector>
        <ownedConnector xmi:id="conn2" xmi:type="uml:Connector">
          <end xmi:id="_conn2_end_1" xmi:type="uml:ConnectorEnd" role="prop2"/>
          <end xmi:id="_conn2_end_2" xmi:type="uml:ConnectorEnd" role="param2" partWithPort="cp1"/>
        </ownedConnector>
        <ownedConnector xmi:id="conn3" xmi:type="uml:Connector">
          <end xmi:id="_conn3_end_1" xmi:type="uml:ConnectorEnd" role="prop3"/>
          <end xmi:id="_conn3_end_2" xmi:type="uml:ConnectorEnd" role="param3" partWithPort="cp1"/>
        </ownedConnector>
        <ownedConnector xmi:id="conn4" xmi:type="uml:Connector">
          <end xmi:id="_conn4_end_1" xmi:type="uml:ConnectorEnd" role="prop4"/>
          <end xmi:id="_conn4_end_2" xmi:type="uml:ConnectorEnd" role="param4" partWithPort="cp1"/>
        </ownedConnector>
      </packagedElement>
      <packagedElement xmi:id="cb1" name="TemperatureControlEquation" xmi:type="uml:Class">
        <ownedRule xmi:type="uml:Constraint" xmi:id="cb1_cr">
          <specification xmi:type="uml:OpaqueExpression" xmi:id="cb1_spec">
            <body>T_current + (Q * t) = T_target</body>
            <language>Math</language>
          </specification>
        </ownedRule>
        <ownedAttribute xmi:id="param1" name="T_current" xmi:type="uml:Port" visibility="public" aggregation="none">
          <type href="http://www.omg.org/spec/SysML/20181001/SysML.xmi#SysML_dataType.Real"/>
        </ownedAttribute>
        <ownedAttribute xmi:id="param2" name="T_target" xmi:type="uml:Port" visibility="public" aggregation="none">
          <type href="http://www.omg.org/spec/SysML/20181001/SysML.xmi#SysML_dataType.Real"/>
        </ownedAttribute>
        <ownedAttribute xmi:id="param3" name="Q" xmi:type="uml:Port" visibility="public" aggregation="none">
          <type href="http://www.omg.org/spec/SysML/20181001/SysML.xmi#SysML_dataType.Real"/>
        </ownedAttribute>
        <ownedAttribute xmi:id="param4" name="t" xmi:type="uml:Port" visibility="public" aggregation="none">
          <type href="http://www.omg.org/spec/SysML/20181001/SysML.xmi#SysML_dataType.Real"/>
        </ownedAttribute>
      </packagedElement>
    </packagedElement>
